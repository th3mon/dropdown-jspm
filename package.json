{
  "name": "dropdown-jspm",
  "version": "1.0.0",
  "scripts": {
    "serveit": "browser-sync start --files 'src/**/*.html, src/**/*.css, src/**/*.js' --server 'src'",
    "buildMode": "jspm bundle startup + dropdown/dropdown --inject",
    "devMode": "jspm unbundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/th3mon/dropdown-jspm.git"
  },
  "author": "Przemyslaw Szelenberger <p.szelenberger@gmail.com> (http://github.com/th3mon)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/th3mon/dropdown-jspm/issues"
  },
  "homepage": "https://github.com/th3mon/dropdown-jspm#readme",
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "jspm": "^0.16.26"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@^3.3.6",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "jquery": "npm:jquery@^2.2.0",
      "json": "github:systemjs/plugin-json@^0.1.0",
      "text": "github:systemjs/plugin-text@^0.0.4"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "clean-css": "npm:clean-css@^3.4.9",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
